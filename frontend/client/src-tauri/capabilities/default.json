{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    {
      "identifier": "http:allow-fetch",
      "allow": [
        {
          "url": "https://*"
        },
        {
          "url": "http://*"
        }
      ],
      "deny": []
    },
    "core:default",
    "http:default",
    "notification:default",
    "sql:default",
    "sql:allow-execute",
    "store:default",
    "fs:default",
    "fs:allow-resource-read-recursive"
  ]
}
